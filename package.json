{
  "name": "brink-daytimer",
  "version": "1.0.0",
  "description": "day analytics to inform behaviour",
  "main": "build/daytimer.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "ui:js": "watchify dev/ui/js/index.cjsx -o ui/scripts/bundle.js -t coffee-reactify -dv",
    "ui:css": "sass -w dev/ui/css/index.sass:ui/css/index.css",
    "server": "nodemon -w 'server' server/index.js",
    "server:build": "cjsx -wo server dev/server",
    "watch": "npm run server:build & npm run ui:js & npm run ui:css",
    "dev": "browser-sync start --proxy='localhost:2000' --files 'ui/**/*.css, ui/**/*.js, dev/ui/jade/**/*.*'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/findthebrink/brink-daytimer.git"
  },
  "keywords": [
    "daytimer",
    "brink",
    "tasks",
    "daily",
    "analytics",
    "help",
    "behaviour",
    "influence",
    "help",
    "make",
    "better",
    "work",
    "ADD",
    "ADHD",
    "assist",
    "executive",
    "executive",
    "function"
  ],
  "author": "Derek Moore <repos@findthebrink.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/findthebrink/brink-daytimer/issues"
  },
  "homepage": "https://github.com/findthebrink/brink-daytimer#readme",
  "dependencies": {
    "body-parser": "^1.14.1",
    "brink-sass": "^0.2.2",
    "cookie-parser": "^1.4.0",
    "express": "^4.13.3",
    "express-session": "^1.12.1",
    "jade": "^1.11.0",
    "jquery": "^2.1.4",
    "mongoose": "^4.2.5",
    "morgan": "^1.6.1",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "weather": "^0.1.0"
  },
  "devDependencies": {
    "coffee-reactify": "^4.0.0"
  }
}
